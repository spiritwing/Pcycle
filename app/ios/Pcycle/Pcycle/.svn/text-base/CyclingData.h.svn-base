#ifndef Pcycle_CyclingData_h
#define Pcycle_CyclingData_h

#import "AppDefaultAndConstValue.h"

@interface CyclingData : NSObject

@property  (nonatomic) float _speed;
@property  (nonatomic) float _power;
@property (retain, nonatomic) TIME_STAMP_TYPE _timeStamp;
@property  (nonatomic) float _longitude;
@property  (nonatomic) float _latitude;
@property  (nonatomic) float _altitude;
@property  (nonatomic) float _climbHeight;
@property  (nonatomic) float _distance;
@property  (nonatomic) float _gradient;
//@property  (nonatomic) BOOL _uploadOrNot;

-(id) initCyclingDataSpeed : (float) speed
                                   Power : (float) power
                                   TimeStamp : (TIME_STAMP_TYPE) timeStamp
                                   Longitude : (float) longitude
                                   Latitude : (float) latitude
                                   Altitude : (float) altitude
                                   ClimbHeight : (float) climbHeight
                                   Distance : (float) distance
                                   Gradient : (float) gradient

;


@end

#endif
